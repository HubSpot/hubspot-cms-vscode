{
  "brackets": [
    ["{%", "%}"],
    ["{{", "}}"],
    ["{#", "#}"]
  ],
  "autoClosingPairs": [
    ["{#", "#}"],
    ["{{", "}}"],
    ["{%", "%}"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["%", "%"],
    ["#", "#"]
  ],
  "comments": {
    "blockComment": ["{#", "#}"]
  },
  "wordPattern": "{%(.*).*%}",
  "indentationRules": {
    "decreaseIndentPattern": ".*{%(.*?end).*%}*.",
    "increaseIndentPattern": ".*{%(?!.*end).*%}.*"
  }
}
